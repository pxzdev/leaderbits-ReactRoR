          - current_user.vacation_modes.order(starts_at: :desc).each do |vm|
            div style="padding: 10px; border: 1px solid #cacaca"
              - format = 'Jan 31 2019 11:59 AM'
              h5= "#{vm.starts_at.in_time_zone(current_user.time_zone).stamp(format)} - #{vm.ends_at.in_time_zone(current_user.time_zone).stamp(format)}"
              blockquote
                = simple_format vm.reason
              - if policy(vm).edit?
                .callout
                  ul.menu.simple
                    - if policy(vm).edit?
                      li
                        a style="cursor: pointer" data-open="exampleModal#{vm.id}" Edit
                        .reveal id="exampleModal#{vm.id}" data-reveal=''
                          = simple_form_for vm, html: { novalidate: false } do |f|
                            .row
                              .small-12.columns style="margin-bottom: 15px"
                                = react_component 'DateRangeSelector', starts_at: vm.starts_at, ends_at: vm.ends_at
                              .small-12.columns
                                = f.input :reason, required: true, label: "What's happening?", input_html: { rows: 6 }

                                = f.submit 'Update', class: "button expanded"

                          button.close-button data-close='' aria-label="Close modal" type="button"
                           span aria-hidden="true" &times;

                    - if policy(vm).destroy?
                      li
                        = link_to vm, method: :delete, title: 'Destroy', class: '', data: { confirm: 'Are you sure you want to delete this item?' } do
                          | Destroy
        - new_vacation_mode = load_new_vacation_mode
        - if new_vacation_mode.present?
          .small-12.large-centered.columns
            = simple_form_for new_vacation_mode, html: { novalidate: false } do |f|
              .row
                .small-12.columns style="margin-bottom: 15px"
                  = react_component 'DateRangeSelector', starts_at: new_vacation_mode.starts_at, ends_at: new_vacation_mode.ends_at
                .small-12.columns
                  = f.input :reason, required: true, label: "What's happening?", input_html: { rows: 6 }

                  = f.submit 'Save', class: "button expanded"
